{
  "npmScope": "zentrumnawi",
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": "*",
    "tslint.json": "*",
    "nx.json": "*",
    "tsconfig.base.json": "*",
    ".eslintrc.json": "*"
  },
  "projects": {
    "ais": {
      "tags": []
    },
    "dive": {
      "tags": []
    },
    "geomat": {
      "tags": []
    },
    "planty": {
      "tags": []
    },
    "solid-core": {
      "tags": ["solid"]
    },
    "solid-glossary": {
      "tags": ["solid"]
    },
    "solid-profile": {
      "tags": ["solid"]
    },
    "solid-quiz": {
      "tags": ["solid"]
    },
    "solid-skeleton": {
      "tags": ["solid"]
    },
    "solid-slideshow": {
      "tags": ["solid"]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test"]
      }
    }
  },
  "affected": {
    "defaultBase": "master"
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "geomat",
  "generators": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  }
}
