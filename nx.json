{
  "npmScope": "zentrumnawi",
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": "*",
    "nx.json": "*",
    "tsconfig.base.json": "*",
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test"],
        "parallel": 1
      }
    }
  },
  "affected": {
    "defaultBase": "master"
  },
  "defaultProject": "geomat",
  "generators": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    }
  }
}
