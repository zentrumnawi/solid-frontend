{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "geomat": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/geomat",
      "sourceRoot": "apps/geomat/src",
      "prefix": "geomat",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/geomat",
            "index": "apps/geomat/src/index.html",
            "main": "apps/geomat/src/main.ts",
            "polyfills": "apps/geomat/src/polyfills.ts",
            "tsConfig": "apps/geomat/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/geomat/src/favicon.ico",
              "apps/geomat/src/assets"
            ],
            "styles": [
              "./node_modules/material-icons/iconfont/material-icons.scss",
              "apps/geomat/src/styles.scss"
            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "libs/solid/glossary/src",
                "libs/solid/skeleton/src",
                "libs/solid/quiz/src",
                "libs/solid/profile/src"
              ]
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/geomat/src/environments/environment.ts",
                  "with": "apps/geomat/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json"
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "apps/geomat/src/environments/environment.ts",
                  "with": "apps/geomat/src/environments/environment.staging.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "geomat:build",
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "geomat:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "geomat:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/geomat/tsconfig.app.json",
              "apps/geomat/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/geomat/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/geomat/jest.config.js",
            "tsConfig": "apps/geomat/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/geomat/src/test-setup.ts"
          }
        }
      }
    },
    "geomat-e2e": {
      "root": "apps/geomat-e2e",
      "sourceRoot": "apps/geomat-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/geomat-e2e/cypress.json",
            "tsConfig": "apps/geomat-e2e/tsconfig.e2e.json",
            "devServerTarget": "geomat:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "geomat:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/geomat-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/geomat-e2e/**"
            ]
          }
        }
      }
    },
    "solid-core": {
      "projectType": "library",
      "root": "libs/solid/core",
      "sourceRoot": "libs/solid/core/src",
      "prefix": "solid-core",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/solid/core/tsconfig.lib.json",
              "libs/solid/core/tsconfig.spec.json"
            ],
            "exclude": [
