        }
      }
    },
    "solid-skeleton": {
      "projectType": "library",
      "root": "libs/solid/skeleton",
      "sourceRoot": "libs/solid/skeleton/src",
      "prefix": "solid-skeleton",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
