              "!libs/solid/core/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/solid/core/jest.config.js",
            "tsConfig": "libs/solid/core/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/solid/core/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "solid-quiz": {
      "projectType": "library",
      "root": "libs/solid/quiz",
